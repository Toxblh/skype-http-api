{
  "name": "skype-http-api",
  "description": "Unofficial Skype API for Node.js via HTTP",
  "version": "0.0.1",
  "license": "MIT",
  "main": "build/lib/index",
  "types": "build/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/toxblh/skype-http-api.git"
  },
  "scripts": {
    "start": "node build/example/test.js",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "posttest": "rm log-*",
    "coverage": "jest --coverage",
    "typedoc": "typedoc",
    "typecover": "type-coverage --detail --ignore-catch",
    "posttypedoc": "touch ./docs/.nojekyll",
    "patch": "npm version patch && git push --follow-tags",
    "minor": "npm version minor && git push --follow-tags",
    "major": "npm version major && git push --follow-tags"
  },
  "keywords": [
    "Skype",
    "Skype-Web",
    "Skype-HTTP"
  ],
  "nyc": {
    "include": [
      "build/test/lib/**/*.js",
      "build/test/lib/**/*.mjs"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".mjs"
    ]
  },
  "@std/esm": {
    "esm": "cjs"
  },
  "bugs": {
    "url": "https://github.com/toxblh/skype-http-api/issues"
  },
  "homepage": "https://github.com/Toxblh/skype-http-api",
  "author": "Anton Palgunov",
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/escape-html": "^1.0.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.8",
    "@types/tough-cookie": "^4.0.0",
    "@types/tunnel": "^0.0.1",
    "dotenv": "^8.2.0",
    "jest": "^26.5.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "async-file": "^2.0.2",
    "big-integer": "^1.6.48",
    "cheerio": "^1.0.0-rc.3",
    "escape-html": "^1.0.3",
    "got": "^11.7.0",
    "incident": "^3.2.1",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.20",
    "tough-cookie": "^4.0.0",
    "tunnel": "^0.0.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  },
  "typeCoverage": {
    "atLeast": 90
  }
}
